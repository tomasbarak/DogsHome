    left: 175px;
    transform: translate(-50%, -50%);
  }

  body,
  html {
    height: 100%;
    margin: 0;
    padding: 0;
  }

  body {
    background: rgb(51, 55, 66);
    background: linear-gradient(90deg, rgba(51, 55, 66, 1) 0%, rgba(30, 32, 37, 1) 100%);
  }

  #dead-face {
    -webkit-filter: drop-shadow(2px 2px 10px rgba(0, 0, 0, .5));
    filter: drop-shadow(2px 2px 10px rgba(0, 0, 0, .5));

    transform: scale(1.5)
  }

  #error-cont {
    position: relative;
    width: calc(100% - 445px);
    height: 75%;
    margin-left: 445px;
    box-sizing: border-box;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-direction: column;
  }

  #error-num{
    font-family: 'Righteous', cursive;
    font-size: 100px;
    color: #fff;
    text-shadow: 2px 2px 10px rgba(0, 0, 0, .5);
  }

  #error-text{
    font-family: 'Righteous', cursive;
    font-size: 30px;
    color: #fff;
    text-shadow: 2px 2px 10px rgba(0, 0, 0, .5);
  }

  #inicio-btn{
    transition: .5s;
    font-family: 'Righteous', cursive;
    font-size: 15px;
    color: #fff;
    background: #079292;
    border: none;
    border-radius: 5px;
    padding: 10px;
    margin-top: 10px;
    cursor: pointer;
    margin-top: 75px;
    text-decoration: none;
  }

  #inicio-btn:hover{
    transition: .25s;
    box-shadow: 0 0 10px rgba(7, 146, 146, .5);
  }
  /*Use screen width*/
  @media screen and (max-width: 800px) {
    #svg-center {
      top: 75px;
      left: 50%;
    }

    #dead-face {
      transform: scale(1.2)
    }
    #error-cont {
      width: calc(100%);
      height: auto;
      margin-left: 0;
      padding-top: 300px;
    }
  }
</style>

