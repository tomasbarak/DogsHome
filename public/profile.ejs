                                Swal.showLoading();
                            }
                        })
                        axios.post(`https://${window.location.host}/create/publication/draft`)
                            .then(function (response) {
                                const data = response.data || {};
                                console.log(data);
                                const id = data.id || '';
                                const redirectPath = data.redirectPath || '';
                                window.location.href = redirectPath;
                            })
                            .catch(function (error) {
                                console.log(error);
                            });
                    }
                </script>
            <% } %>
