{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpx nuxt build",
    "dev": "pnpx nuxt dev --https --ssl-cert ./certs/ssl/certificate.crt --ssl-key ./certs/ssl/private.key",
    "generate": "pnpx nuxt generate",
    "preview": "pnpx nuxt preview",
    "preinstall": "pnpx only-allow pnpm",
    "postinstall": "pnpx nuxt prepare",
    "prod": "NITRO_PORT=8443 NITRO_SSL_CERT=`cat ./certs/ssl/certificate.crt` NITRO_SSL_KEY=`cat ./certs/ssl/private.key` node .output/server/index.mjs"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/google-fonts": "^3.1.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@sidebase/nuxt-auth": "^0.6.0-beta.6",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.12.10",
    "nuxt": "^3.7.4",
    "nuxt-icon": "^0.5.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "@nuxt/components": "^2.2.1",
    "@nuxtjs/auth": "^4.9.1",
    "@pinia/nuxt": "^0.5.1",
    "@types/ejs": "^3.1.3",
    "@types/firebase": "^3.2.1",
    "@types/lodash": "^4.17.7",
    "@types/nodemailer": "^6.4.11",
    "@vue": "link:@types/@vue",
    "auth": "link:@types/firebase/auth",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "firebase": "^10.4.0",
    "firebase-admin": "^11.11.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "nodemailer": "^6.9.5",
    "pinia": "^2.1.7",
    "reactivity": "link:@types/@vue/reactivity",
    "sweetalert2": "^11.10.1",
    "vue-material-design-icons": "^5.2.0"
  },
  "overrides": {
    "vue": "latest"
  }
}
